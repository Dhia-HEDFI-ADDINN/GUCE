      "name": "GUCE",
      "path": "/GUCE",
      "subGroups": [
        { "name": "Agents", "path": "/GUCE/Agents" },
        { "name": "Superviseurs", "path": "/GUCE/Superviseurs" },
        { "name": "Direction", "path": "/GUCE/Direction" }
      ]
    },
    {
      "name": "Administration",
      "path": "/Administration",
      "subGroups": [
        { "name": "AdminFonctionnel", "path": "/Administration/AdminFonctionnel" },
        { "name": "AdminTechnique", "path": "/Administration/AdminTechnique" },
        { "name": "SuperAdmin", "path": "/Administration/SuperAdmin" }
      ]
    }
  ],
  "clients": [
    {
      "clientId": "guce-cameroun",
      "name": "GUCE Cameroun - Interface Unique",
      "description": "Interface unique de l'instance GUCE Cameroun - Acces a tous les modules (e-Force, e-Gov, e-Business, e-Payment, Procedure Builder, Admin Local) via navigation interne selon les roles de l'utilisateur",
      "enabled": true,
      "publicClient": true,
      "rootUrl": "https://guce-cameroun.com",
      "baseUrl": "/",
      "redirectUris": [
        "http://localhost:4200/*",
        "https://guce-cameroun.com/*",
        "https://*.guce.cm/*"
      ],
      "webOrigins": [
        "http://localhost:4200",
        "https://guce-cameroun.com",
        "https://*.guce.cm"
      ],
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "protocol": "openid-connect",
      "defaultClientScopes": ["openid", "profile", "email", "roles"],
      "attributes": {
        "post.logout.redirect.uris": "http://localhost:4200/*##https://guce-cameroun.com/*"
      }
    },
    {
      "clientId": "guce-api",
      "name": "API GUCE Instance",
      "description": "Client pour les APIs backend de l'instance - Tous les microservices",
      "enabled": true,
      "publicClient": false,
      "secret": "guce-api-secret",
      "bearerOnly": true,
      "protocol": "openid-connect",
      "defaultClientScopes": ["openid", "profile", "email", "roles"]
    },
    {
      "clientId": "hub-agent",
      "name": "Agent Hub",
      "description": "Client pour la communication avec le Hub E-GUCE 3G (monitoring, updates, templates sync)",
      "enabled": true,
      "publicClient": false,
      "secret": "hub-agent-secret",
      "serviceAccountsEnabled": true,
      "protocol": "openid-connect",
      "defaultClientScopes": ["openid", "profile", "email", "roles"]
    }
  ],
