  "groups": [
    {
      "name": "Operateurs",
      "path": "/Operateurs",
      "subGroups": [
        { "name": "ImportateursExportateurs", "path": "/Operateurs/ImportateursExportateurs" },
        { "name": "Declarants", "path": "/Operateurs/Declarants" },
        { "name": "Commissionnaires", "path": "/Operateurs/Commissionnaires" }
