{
  "name": "guce-instance-template",
  "version": "1.0.0",
  "description": "GUCE Instance Template - Interface Unique Single-Tenant (e-Force, e-Gov, e-Business, e-Payment, Procedure Builder, Admin)",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/cdk": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/material": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "keycloak-angular": "^16.0.0",
    "keycloak-js": "^23.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.0",
    "zone.js": "~0.14.0",
    "chart.js": "^4.4.0",
    "ng2-charts": "^6.0.0",
    "bpmn-js": "^17.0.0",
    "dmn-js": "^16.0.0",
    "@bpmn-io/form-js": "^1.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.0",
    "@angular/cli": "^18.0.0",
    "@angular/compiler-cli": "^18.0.0",
    "@types/node": "^20.0.0",
    "typescript": "~5.4.0"
  },
  "guce": {
    "template": true,
    "version": "3.0.0",
    "generatedBy": "E-GUCE 3G Generator Hub",
    "variables": {
      "INSTANCE_CODE": "{{INSTANCE_CODE}}",
      "INSTANCE_NAME": "{{INSTANCE_NAME}}",
      "INSTANCE_DOMAIN": "{{INSTANCE_DOMAIN}}",
      "PRIMARY_COLOR": "{{PRIMARY_COLOR}}",
      "SECONDARY_COLOR": "{{SECONDARY_COLOR}}",
      "KEYCLOAK_URL": "{{KEYCLOAK_URL}}",
      "KEYCLOAK_REALM": "{{KEYCLOAK_REALM}}",
      "API_URL": "{{API_URL}}"
    }
  }
}
